page {
  font-size: 32rpx;
  line-height: 1.6;
  font-family: Avenir, Helvetica, sans-serif;
  width:100%;
  margin: 0 auto;
}

.banner {
  color: white;
  text-align: center;
  height: 100vh;
  background-size: cover !important;
  background-position: 0%;
  display: flex;
  align-items: center;
  justify-content: center;
}

navigator {
  text-decoration: none;
  border: 1px solid white;
  border-radius:2px;
  padding: 10px;
  width:200px;
  text-align: center;
  margin: 0 auto;
}